  "scripts": {
    "docker:all": "npm run docker:local && npm run docker:staging && npm run docker:production",
    "docker:local": "docker-compose -f docker-compose.local.yml up --build --exit-code-from test-automation",
    "docker:production": "docker-compose -f docker-compose.production.yml up --build --exit-code-from test-automation",
    "docker:staging": "docker-compose -f docker-compose.staging.yml up --build --exit-code-from test-automation",
    "install:browsers": "npx playwright install --with-deps",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:local": "NODE_ENV=local playwright test",
    "test:production": "NODE_ENV=production playwright test",
    "test:report": "playwright show-report",
    "test:staging": "NODE_ENV=staging playwright test",
    "test:ui": "playwright test --ui"
  }
